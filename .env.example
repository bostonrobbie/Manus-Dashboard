# Manus-compatible environment defaults
PORT=3001
# MySQL/TiDB-compatible connection string used by Drizzle and the API server
# Example local connection (no secrets committed):
DATABASE_URL=mysql://user:password@localhost:3306/manus_dashboard
NODE_ENV=development
VITE_API_URL=http://localhost:3001
# Optional auth/test placeholders (safe defaults are injected automatically when NODE_ENV=test)
MANUS_MODE=false
MANUS_JWT_SECRET=replace-me
MANUS_PUBLIC_KEY_URL=https://example.com/mock-key
AUTH_DEBUG_ENABLED=false
