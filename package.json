{
  "name": "manus-dashboard",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "tsc": "pnpm -w tsc",
    "build": "pnpm -r run build",
    "build:client": "pnpm --filter client run build",
    "build:server": "pnpm --filter server run build",
    "start": "pnpm --filter server run start",
    "preview": "pnpm --filter client run preview",
    "migrate": "pnpm --filter drizzle run migrate"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "typescript": "^5.4.5"
  }
}
