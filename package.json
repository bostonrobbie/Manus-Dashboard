{
  "name": "manus-dashboard",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "lint": "pnpm exec eslint client/src server/src shared --ext .ts,.tsx",
    "typecheck": "pnpm -r run typecheck",
    "build": "pnpm -r run build",
    "build:client": "pnpm --filter client run build",
    "build:server": "pnpm --filter server run build",
    "start": "pnpm --filter server run start:prod",
    "preview": "pnpm --filter client run preview",
    "test:server": "pnpm --filter server test",
    "test:all": "pnpm run lint && pnpm run typecheck && pnpm --filter server test && pnpm --filter server build && pnpm --filter client build",
    "migrate": "pnpm --filter drizzle run migrate",
    "db:seed:demo": "tsx server/scripts/seed-demo-data.ts",
    "db:seed:large": "tsx server/scripts/load-large-dataset.ts",
    "smoke:test": "pnpm --filter server run smoke:test"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.12.0",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "patchedDependencies": {
      "drizzle-kit@0.22.8": "patches/drizzle-kit@0.22.8.patch"
    }
  }
}
