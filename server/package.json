{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node -r tsconfig-paths/register dist/server/index.js",
    "start:prod": "pnpm run build && node -r tsconfig-paths/register dist/server/index.js",
    "tsc": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "pnpm exec eslint . --ext .ts",
    "test": "NODE_OPTIONS=\"--require tsconfig-paths/register\" tsx --test ./tests/webhook.test.ts ./tests/seedParsing.test.ts",
    "test:all": "NODE_OPTIONS=\"--require tsconfig-paths/register\" tsx --test ./tests/**/*.ts",
    "migrate": "tsx ../scripts/migrate.ts",
    "load:trades": "tsx ./scripts/load-real-trades.ts",
    "smoke": "tsx ./scripts/smoke-test-portfolio.ts",
    "smoke:test": "tsx ./scripts/smoke-test.ts"
  },
  "dependencies": {
    "@trpc/server": "^11.0.0-rc.660",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.0",
    "tsconfig-paths": "file:./tsconfig-paths",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@trpc/client": "^11.0.0-rc.660",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.10",
    "@types/supertest": "^6.0.3",
    "superjson": "^2.2.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6"
  }
}
