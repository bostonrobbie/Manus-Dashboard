{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node -r tsconfig-paths/register dist/server/src/index.js",
    "tsc": "tsc -p tsconfig.json",
    "test": "node --test -r ts-node/register -r tsconfig-paths/register tests/**/*.test.ts",
    "migrate": "tsx ../scripts/migrate.ts",
    "load:trades": "tsx ./scripts/load-real-trades.ts",
    "smoke": "tsx ./scripts/smoke-test-portfolio.ts"
  },
  "dependencies": {
    "@trpc/server": "^11.0.0-rc.660",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "tsconfig-paths": "file:./tsconfig-paths",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.10",
    "@types/pg": "^8.15.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6"
  }
}
